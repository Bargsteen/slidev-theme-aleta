{
  "name": "slidev-theme-aleta",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build ${npm_config_file}.md",
    "dev": "slidev ${npm_config_file}.md --open",
    "export": "slidev export ${npm_config_file}.md",
    "screenshot": "slidev export ${npm_config_file}.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.0-beta.4",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.0-beta.4"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Gilroy",
        "mono": "Source Code Pro",
        "local": "Gilroy",
        "weights": "300,400,500,600,700"
      }
    }
  }
}